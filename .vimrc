filetype off
call pathogen#infect()
filetype plugin indent on
set nocompatible                                                                
set backspace=indent,eol,start                                                  
syntax on                                                                       
set tabstop=4 softtabstop=4  si ai                                              
set shiftwidth=4                                                                
color murphy                                                                    
color evening                                                                   
set hlsearch
set incsearch    
set number
set ruler
set expandtab
set autochdir
